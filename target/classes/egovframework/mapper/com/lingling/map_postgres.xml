<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="LingLing">


	<!-- 리스트 출력 -->
	<select id="list" resultType="taerogramming.ex.vo.ExVO">
  		select * from mapmap order by num desc
	</select>
	<!-- 리스트 출력 -->

	<!-- 등록 -->
	<insert id="create">
		insert into mapmap (title, addr, xxx, yyy) 
        values (#{title}, #{addr}, #{xxx}, #{yyy})
	</insert>
	<!-- 등록 -->








</mapper>